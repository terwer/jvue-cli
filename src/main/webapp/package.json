{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist && rimraf ssrdist",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --ext .js,.vue --ignore-path .gitignore . --fix --color",
    "inspect": "vue inspect",
    "build:entry-client": "vue-cli-service build --dest ssrcdist src/ssr/entry-client.js --report-json",
    "test:entry-client": "node ./tests/ssr/test-entry-client.js",
    "build:entry-server": "vue-cli-service build --dest ssrsdist src/ssr/entry-server.js --report-json",
    "test:entry-server": "node ./tests/ssr/test-entry-server.js",
    "build:ssr": "./build/build.ssr.sh"
  },
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.11",
    "vue": "^2.6.3",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/node": "^7.2.2",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.6.3",
    "webpack-cli": "^3.2.1"
  }
}
