{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "inspect": "vue inspect",
    "lint:src": "eslint ./src/*.js --fix --color",
    "lint:ssr": "eslint ./src/ssr/*.js --fix --color",
    "lint:app": "eslint ./src/App.vue --fix --color",
    "lint:components": "eslint ./src/components --ext .vue --fix --color",
    "lint:views": "eslint ./src/views --ext .vue --fix --color",
    "lint:build": "eslint ./build/*.js --fix --color",
    "build:entry-client": "vue-cli-service build --dest ssrcdist src/ssr/entry-client.js --report-json",
    "test:entry-client": "node ./tests/ssr/test-entry-client.js",
    "build:entry-server": "vue-cli-service build --dest ssrsdist src/ssr/entry-server.js --report-json",
    "test:entry-server": "node ./tests/ssr/test-entry-server.js",
    "build:ssr": "./build/build.ssr.sh"
  },
  "dependencies": {
    "vue": "^2.5.22",
    "vue-router-nashorn": "file:./vender/lib/vue-router-nashorn",
    "vue-server-renderer": "^2.5.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/node": "^7.2.2",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.22",
    "webpack-cli": "^3.2.1"
  }
}
